<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="50.0" prefWidth="824.0" styleClass="Hbox-detail" stylesheets="@../CSS/Produits.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GuiFT.MesProduitItemsController">
   <children>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="120.0" styleClass="Hbox-detail" stylesheets="@../CSS/Produits.css">
         <children>
            <Label fx:id="id" text="Label" visible="false" />
            <Label fx:id="imgLabel" prefHeight="3.0" prefWidth="0.0" text="Label" visible="false" />
            <ImageView fx:id="img" fitHeight="50.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" styleClass="Hbox-img">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <Pane prefHeight="200.0" prefWidth="10.0" />
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="120.0">
         <children>
            <Label fx:id="titre" text="Titre" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Pane prefHeight="200.0" prefWidth="10.0" />
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="120.0">
         <children>
            <Label fx:id="poids" text="Poids" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Pane prefHeight="200.0" prefWidth="10.0" />
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="120.0">
         <children>
            <Label fx:id="date" text="Date" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Pane prefHeight="200.0" prefWidth="10.0" />
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="120.0">
         <children>
            <Label fx:id="prix" text="Prix" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Pane prefHeight="200.0" prefWidth="10.0" />
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="150.0">
         <children>
            <Button fx:id="UpdateButton" mnemonicParsing="false" onAction="#Update" styleClass="Hbox-button" stylesheets="@../CSS/Produits.css" text="Update" />
            <Pane prefWidth="5.0" />
            <Button fx:id="DeleteButton" mnemonicParsing="false" onAction="#Delete" styleClass="Hbox-button" text="Delete" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" top="10.0" />
   </padding>
</HBox>
